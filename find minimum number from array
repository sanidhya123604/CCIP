#include <iostream>
#include<string.h>


using namespace std;

int main()
{
    int n;
    cout<<"enter the size of an array:";
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int minimum = 100;
    for(int i= 0;i<n;i++)
    {
        minimum = min(arr[i],minimum); 
    }
    cout<<minimum;
    return 0;
}
