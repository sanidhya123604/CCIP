#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n,i,j; 
    cin>>n;
    for(i=1;i<=n;i++)
    {
        char ch='A';
        for(j=1;j<=i;j++)
        {
            cout<<ch;
            ch=ch+1;
        }
    cout<<"\n";    
    }
    return 0;
}
