#include <iostream>
#include<string.h>


using namespace std;

int main()
{
    int n,r,sum=0;
    cout<<"enter the value of n:";
    cin>>n;
    int count =0;
    for(int i=2;i<n/2;i++)
    {
        if(n%i==0)
        {
            count = count +1;
        }
    }
    if(count ==0)
    {
        cout<<"prime number";
    }
    else
    {
        cout<<"not prime number";
    }

    return 0;
}
