#include <iostream>
#include<string.h>


using namespace std;

int main()
{
    string s;
    cout<<"enter the string";
    getline(cin,s);
    for(int i=0;i<s.length()/2;i++)
    {
        if(s[i]!=s[s.length()-i-1])
        {
            cout<<"not palidrome";
            break;
        }
    }
    cout<<"palidrome";

    return 0;
}
